{
  "name": "@jasmith79/sequable",
  "version": "0.0.1",
  "description": "Library functions for working with generators",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run-script build-cjs && npm run-script build-mjs",
    "build-cjs": "tsc && npm run-script rename-cjs",
    "build-mjs": "tsc --module esnext && npm run-script rename-mjs",
    "format": "prettier . --write",
    "lint": "eslint ./src",
    "rename-cjs": "for f in ./dist/*.js; do mv -- \"$f\" \"${f%.js}.cjs\"; done",
    "rename-mjs": "for f in ./dist/*.js; do mv -- \"$f\" \"${f%.js}.mjs\"; done",
    "test": "jest --silent=false"
  },
  "keywords": [
    "generators",
    "es-6",
    "functional-programming"
  ],
  "author": "jasmith79@github.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
