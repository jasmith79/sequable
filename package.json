{
  "name": "@jasmith79/sequable",
  "version": "0.0.1",
  "description": "Library functions for working with generators",
  "sideEffects": false,
  "type": "module",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "typings": "./esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./cjs/index.cjs",
      "import": "./esm/index.js"
    },
    "./take": {
      "require": "./cjs/take.cjs",
      "import": "./esm/take.js"
    },
    "./partition": {
      "require": "./cjs/partition.cjs",
      "import": "./esm/partition.js"
    },
    "./map": {
      "require": "./cjs/map.cjs",
      "import": "./esm/map.js"
    },
    "./filter": {
      "require": "./cjs/filter.cjs",
      "import": "./esm/filter.js"
    },
    "./sequable": {
      "require": "./cjs/sequable.cjs",
      "import": "./esm/sequable.js"
    },
    "./zip": {
      "require": "./cjs/zip.cjs",
      "import": "./esm/zip.js"
    }
  },
  "scripts": {
    "build": "npm run-script build-cjs && npm run-script build-esm/",
    "build-cjs": "tsc && npm run-script change-requires && npm run-script rename-cjs",
    "build-esm/": "tsc --module esnext --outDir ./esm && fix-esm-import-path ./esm/index.js",
    "rename-cjs": "./scripts/rename-cjs-files",
    "change-requires": "./scripts/rename-requires",
    "clean": "rm -rf ./esm && rm -rf ./cjs",
    "format": "prettier . --write",
    "lint": "eslint ./src",
    "test": "jest --silent=false"
  },
  "keywords": [
    "generators",
    "es-6",
    "functional-programming"
  ],
  "author": "jasmith79@github.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "fix-esm-import-path": "^1.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
